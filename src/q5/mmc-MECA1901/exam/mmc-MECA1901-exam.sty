\newcommand{\mypar}[1]{\noindent\textbf{#1}}
\DeclareMathOperator{\trace}{tr}

% Des super macros :)
\newcommand{\EvalAt}[1]{\biggr\rvert_{#1}} % pour faire la barre d'evaluation en un point (p. ex. \EvalAt{x=0}.
\newcommand{\Deriv}[2]{\frac{\mathrm{d}#1}{\mathrm{d}#2}} % Pour faire des fractions derivees sans trops se casser la tete :) (p. ex. \Deriv{y(t}{t})
\newcommand{\DerivEvalAt}[3]{\Deriv{#1}{#2}\EvalAt{#3}} % Combo des 2 precedant (p.ex. \DerivEvalAt{y(t)}{t}{t=0})
\newcommand{\Integr}[4]{\int_{#1}^{#2}#3\mathrm{d}#4} % Ecrire des integrales rapidement (p.ex. \Integr{a}{b}{f(x)}{d})
\newcommand{\PDeriv}[2]{\frac{\partial#1}{\partial#2}} % Pour faire des fractions derivees partielle sans trops se casser la tete :) (p. ex. \PDeriv{y(t}{t})
\newcommand{\PTDeriv}[2]{\frac{\mathrm{D}#1}{\mathrm{D}#2}} % Pour faire des fractions derivees partielle en dérivée matérielle sans trops se casser la tete :) (p. ex. \PTDeriv{y(t}{t})
\newcommand{\Base}[1]{\hat{\mathbf{e}}_{#1}} % Pour les vecteurs de base (MMC) par exemple \Base{1},\Base{2} et \Base{3}
\newcommand{\Norm}[1]{\hat{\mathbf{n}}_{#1}}
\newcommand{\uni}[1]{\mathbf{\hat{#1}}} % Pour les vecteurs unitaires sans indice


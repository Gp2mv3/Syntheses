% commande interne teste le spin
\newcommand{\testspin}[1]{%
	\ifthenelse{\equal{#1}{updown}}{$\uparrow$ $\downarrow$}{%
		\ifthenelse{\equal{#1}{up}}{$\uparrow$}{%
			\ifthenelse{\equal{#1}{down}}{$\downarrow$}{%
			}%
		}%
	}%
}

% style OA
\tikzstyle{OAbox} = [draw,thick,minimum height=1cm,minimum width=1cm]

% -----------------------------------------------------------------------------
% boite quantique ns : \OAboxs[3]
% -----------------------------------------------------------------------------
% cree une case quantique
% parametres #1 : positions
%            #2 : remplissage = up, down ou updown
%            #3 : nom
% -----------------------------------------------------------------------------
\newcommand{\OAboxs}[3] {
    \path #1 node[OAbox] (OAs) {\Large \testspin{#2}} ;
    \node[anchor=south] at (OAs.north) {#3};
}

% -----------------------------------------------------------------------------
% boite quantique np : \OAboxp[5]
% -----------------------------------------------------------------------------
% cree trois cases quantiques accolees
% parametres #1   : positions
%            #2-4 : remplissage = up, down ou updown
%            #5   : nom
% -----------------------------------------------------------------------------
\newcommand{\OAboxp}[5]{
    \path #1     node[OAbox]       {\Large \testspin{#2}}
        ++ (1,0) node[OAbox] (OAp) {\Large \testspin{#3}}
        ++ (1,0) node[OAbox]       {\Large \testspin{#4}} ;
    \node[anchor=south] at (OAp.north) {#5};
}

% -----------------------------------------------------------------------------
% boite quantique nd : \OAboxp[7]
% -----------------------------------------------------------------------------
% cree trois cases quantiques accolees
% parametres #1   : positions
%            #2-6 : remplissage = up, down ou updown
%            #7   : nom
% -----------------------------------------------------------------------------
\newcommand{\OAboxd}[7]{
    \path #1     node[OAbox]       {\Large \testspin{#2}}
	++ (1,0) node[OAbox]       {\Large \testspin{#3}}
	++ (1,0) node[OAbox] (OAd) {\Large \testspin{#4}}
	++ (1,0) node[OAbox]       {\Large \testspin{#5}}
	++ (1,0) node[OAbox]       {\Large \testspin{#6}} ;
    \node[anchor=south] at (OAd.north) {#7};
}


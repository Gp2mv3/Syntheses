% -----------------------------------------------------------------------------
% commande \drawLevel[3]
% -----------------------------------------------------------------------------
% trace un niveau et rajoute des noeuds
% Parameters: #1 (optional) Nombre de d'electron, par defaut 0
%             #2 position du point a gauche de l'orbitale
%             #3 base du nom du noeud : donne les label left#3 et right#3 et middle#3
% -----------------------------------------------------------------------------
\newcommand{\drawLevel}[3][no]{%
    \draw[levelstyle]
        #2 node (left#3) {} -- ++ (2,00)
        node (right#3) {}
        node[pos=0.5] (middle#3) {}
        node[pos=0.3] (pos1) {}
        node[pos=0.7] (pos2) {} ;

    \ifthenelse{\equal{#1}{updown}}{
        \draw[drawOAelectron] (pos1.center) ++(0,-0.5) -- ++(0,1) ;
        \draw[drawOAelectron] (pos2.center) ++(0, 0.5) -- ++(0,-1) ;
    }{
        \ifthenelse{\equal{#1}{up}}{
            \draw[drawOAelectron] (middle#3.center) ++(0,-0.5) -- ++(0,1) ;
        }{
            \ifthenelse{\equal{#1}{down}}{
                \draw[drawOAelectron] (posm.center) ++(0, 0.5) -- ++(0,-1) ;
            }{
            }
        }
    }
}


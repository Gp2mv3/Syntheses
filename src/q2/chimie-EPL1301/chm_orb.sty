% *
% * Dessins d'orbitales atomiques avec tikz
% *
% *     a partir de :
% *     http://www.texample.net/tikz/examples/atoms-and-orbitals/
% *     Berteun Damman, Arne Rohrs
% *
% * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *

\pgfdeclarelayer{background}
\pgfdeclarelayer{main}
\pgfdeclarelayer{foreground}
\pgfsetlayers{background,main,foreground}

% style general pour les orbitales
\tikzstyle{orbital}=[thick,fill opacity=.60]

% la commande \defOrbitalCurve permet de definir les points necessaires pour le tracer de
% l'orbitale. C'est une commande ``interne''. A priori elle n'est pas utile en dehors des
% macros definies ici.
\def\orbheight{1.2}
\def\orbwidth{.6}
\newcommand{\defOrbitalCurve}{
    % These points define the curve for the orbital.
    \coordinate (c1) at (-\orbwidth, .6 * \orbheight);
    \coordinate (c2) at (-\orbwidth, \orbheight);
    \coordinate (c3) at (\orbwidth, \orbheight);
    \coordinate (c4) at (\orbwidth, .6 * \orbheight);
    \coordinate (top) at (0,\orbheight);
}

% -----------------------------------------------------------------------------
% commande \orbital[4]
% -----------------------------------------------------------------------------
% Parameters: #1 (optional) Orbital color (no mixing color ie. blue!50 is forbiden, put blue only)
%             #2 Rotation of the orbital
%             #3 Coordinate where the orbital should be attached
%             #4 Number of electrons to draw in the orbital
% -----------------------------------------------------------------------------
\renewcommand{\orbital}[4][blue]{
    % definit le tracer de l'orbitale
    \begin{scope}[rotate=#2,shift=(#3)]
        \defOrbitalCurve

        %Coordinates of the electrons
        \coordinate (e1) at (0, 0.45*\orbheight);
        \coordinate (e2) at (0, 0.75*\orbheight);
    \end{scope}

    % These are drawn on a background layer, so orbitals
    % can overlap without covering the electrons, which
    % visualises the role electrons play in chemical bonds.
    \begin{pgfonlayer}{background}
        \draw[orbital,fill=#1!66,draw=#1] (#3) .. controls (c1) and (c2) .. (top) ..
            controls (c3) and (c4) .. (#3);
    \end{pgfonlayer}

    % Draw the electrons
    \ifnum#4>0
        \foreach \n in {1,...,#4} {
            \shade[ball color=#1] (e\n) circle (1mm);
        }
    \fi
}

% -----------------------------------------------------------------------------
% commande \atom[4]
% -----------------------------------------------------------------------------
% This allows to quickly place an atom.
% Parameters: #1 (Optional) Name of the center node
%             #2 Text for the center node
%             #3 Atom color
%             #4 orbital parameters : A list of color/rotation-angle/anchor/number of electrons
% -----------------------------------------------------------------------------
\tikzstyle{atomcore}=[shape=circle,thick,minimum size=7mm,inner sep=0pt]
\newcommand{\Atom}[4][AtomNode]{
  \node[atomcore,draw=#3!40,color=#3!70!gray,fill=#3!20] (#1) {\large#2};
  \foreach \color/\ang/\anchor/\n in {#4} {
      \orbital[\color]{\ang}{#1.\anchor}{\n}
  }
}

% -----------------------------------------------------------------------------
% Dessins d'orbitales atomiques de type p
%       on considere que x et y sont dans le plan de la feuille
%       \orbitalP[3]   pour npx, npy
%       \orbitalPz[3]  pour npz
%       \orbitalS[3]   pour ns
% -----------------------------------------------------------------------------

% shading pour les OA p
\pgfdeclareradialshading{shadeOAp}{\pgfpoint{10bp}{12bp}}
{color(0bp)=(blue!15!white); color(9bp)=(blue!75!white);
color(18bp)=(blue!70!black); color(25bp)=(blue!50!black); color(50bp)=(black)}

% -----------------------------------------------------------------------------
% commande \orbitalP[3]
% -----------------------------------------------------------------------------
% dessine une OA p dans le plan (px ou py)
% parametres #1 : (optionel) facteur de scaling
%            #2 : position du centre de l'OA
%            #3 : rotation de l'OA
% -----------------------------------------------------------------------------
\newcommand{\orbitalP}[3][0.75]{
    \begin{scope}[scale=#1,rotate=#3]
        \begin{scope}[rotate=270,shift=(#2)]
            \defOrbitalCurve
        \end{scope}
        \draw[orbital,fill=black!80,draw=black,shading=shadeOAp] (#2) .. controls (c1) and
            (c2) .. (top) ..  controls (c3) and (c4) .. (#2);

        \begin{scope}[rotate=90,shift=(#2)]
            \defOrbitalCurve
        \end{scope}
        \draw[orbital,fill=black!10,draw=black] (#2) .. controls (c1) and (c2) .. (top) ..
            controls (c3) and (c4) .. (#2);
    \end{scope}
}

% -----------------------------------------------------------------------------
% commande \orbitalPz[2]
% -----------------------------------------------------------------------------
% dessine une OA p perpendiculaire a la feuille (pz)
% parametres #1 : (optionel) facteur de scaling
%            #2 : position du centre de l'OA
% -----------------------------------------------------------------------------
\newcommand{\orbitalPz}[2][1.]{
    \node[xshift=-0.5mm,yshift=-0.5mm] at (#2) (circle1) {} ;
    \node[xshift=0.5mm,yshift=0.5mm] at (#2) (circle2) {} ;
    \draw[orbital,fill=black!10,draw=black] (circle2.center) circle (.35*#1) ;
    \draw[fill=white] (circle1.center) circle (.35*#1) ;
    \draw[orbital,fill=black!80,draw=black,shading=shadeOAp] (circle1.center) circle (.35*#1) ;
}

% -----------------------------------------------------------------------------
% commande \orbitalS[3]
% -----------------------------------------------------------------------------
% Dessins d'orbitales atomiques de type s
%       #1 : (optionel) p pour bleu m pour blanc
%       #2 : position, soit x,y soit nom d'un noeud
%       #3 : facteur de scaling sur le rayon
% -----------------------------------------------------------------------------
\newcommand{\orbitalS}[3][p]{
    \ifthenelse{\equal{#1}{p}}{
        \draw[fill opacity=.60,shading=shadeOAp] (#2) circle (0.5*#3) ;
    }{
        \ifthenelse{\equal{#1}{m}}{
            \draw[fill opacity=.60,ball color=black!10] (#2) circle (0.5*#3) ;
        }{
            \draw[fill opacity=.60,ball color=black!10] (#2) circle (0.5*#3) ;
        }
    }
}

